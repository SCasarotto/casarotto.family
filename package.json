    "@vercel/node": "^2.15.10",
    "ariakit": "2.0.0-next.43",
    "date-fns": "^2.30.0",
    "react": "^18.3.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.3.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/create-config": "^0.4.6",
    "@remix-run/dev": "^1.19.3",
